mv stamp_stubs.txt original_stamp_stubs.txt
cat original_stamp_stubs.txt stubs.txt > stamp_stubs.txt
jar uf ../../../apache-tomcat-6.0.35/webapps/ROOT/android/stamp.android.jar stamp_stubs.txt
