    FlowComp(s,srccl2,s1,sinkcl2), \
    LCL(src2,srccl2), DeviceId(src2), LCL(sink2,sinkcl2), Internet(sink2), \
    FlowComp(s,srccl3,s1,sinkcl3), \
    LCL(src3,srccl3), SimSerial(src3), LCL(sink3,sinkcl3), Internet(sink3), \
    s=s1.


